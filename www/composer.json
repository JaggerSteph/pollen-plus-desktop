{
    "name": "sphinx/framework",
    "description": "Simple PHP mini-Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Piou Hassan",
            "email": "piouhassan@gmail.com"
        }
    ],
    "require": {
        "twig/twig" : "2.4",
        "nesbot/carbon": "~1.21",
        "illuminate/database": "^5.7",
        "illuminate/events": "^5.7",
        "michelf/php-markdown": "^1.8",
        "psr/http-server-middleware": "^1.0",
        "guzzlehttp/psr7": "^1.4",
        "intervention/image": "^2.4",
        "symfony/translation": "^4.2",
        "zendframework/zend-diactoros": "1.7",
        "woohoolabs/harmony": "^6.0",
        "zendframework/zend-httphandlerrunner": "^1.0",
        "nikic/fast-route": "^1.3",
        "grafikart/psr15-csrf-middleware": "^1.0",
        "league/container": "^3.2",
        "filp/whoops": "^2.3",
        "middlewares/whoops": "^1.2",
        "swiftmailer/swiftmailer": "^6.1",
      "ext-json": "*",
        "apfelbox/php-file-download": "^2.1",
        "spipu/html2pdf": "^5.2",
        "dompdf/dompdf": "^0.8.3"
    },
  "autoload" : {
    "psr-4" : {
      "App\\" : "app",
      "Akuren\\" : "packages",
      "AkConfig\\" : "config"
    },

    "files" : [
      "app/helpers.php",
      "config/app.php",
      "config/database.php"
    ],

    "classmap" : [
      "app/Models"
    ]
  },
    "require-dev": {
        "robmorgan/phinx": "^0.10.6",
        "fzaninotto/faker": "^1.8"
    }
}
